{
    "name": "@wq/template",
    "version": "1.2.0",
    "description": "Render Mustache templates and inject as jQuery Mobile pages",
    "main": "dist/index.js",
    "module": "dist/index.es.js",
    "scripts": {
        "test": "cd ../../ && npm run jest packages/template",
        "build": "cd ../../ && npm run rollup -- -c packages/template/rollup.config.js",
        "prettier": "cd ../../ && npm run prettier -- --write packages/template/{,src/,src/__tests__/}*.js{,on}",
        "lint": "cd ../../ && npm run eslint packages/template/{,src/,src/__tests__/}*.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/wq/wq.app.git"
    },
    "keywords": [
        "wq",
        "template",
        "mustache",
        "jquery-mobile"
    ],
    "author": "S. Andrew Sheppard",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/wq/wq.app/issues"
    },
    "homepage": "https://wq.io/wq.app",
    "dependencies": {
        "mustache": "^3.1.0"
    }
}
