{
    "name": "@wq/leaflet",
    "version": "1.2.0-beta.1",
    "description": "Leaflet integration for @wq/map",
    "main": "dist/index.js",
    "module": "dist/index.es.js",
    "scripts": {
        "test": "cd ../../ && npm run jest packages/leaflet -- --moduleDirectories node_modules/@wq/react/node_modules node_modules",
        "build": "cd ../../ && npm run rollup -- -c packages/leaflet/rollup.config.js",
        "prettier": "cd ../../ && npm run prettier -- --write packages/leaflet/{,src/,src/*/,src/*/*/}*.js{,on}",
        "lint": "cd ../../ && npm run eslint packages/leaflet/{,src/,src/*/,src/*/*/}*.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/wq/wq.app.git"
    },
    "keywords": [
        "wq",
        "leaflet",
        "map",
        "gis"
    ],
    "author": "S. Andrew Sheppard",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/wq/wq.app/issues"
    },
    "homepage": "https://wq.io/wq.app",
    "dependencies": {
        "@wq/map": "^1.2.0-beta.1",
        "@wq/react": "^1.3.0-alpha.0",
        "esri-leaflet": "^2.3.0",
        "leaflet": "^1.5.1",
        "leaflet-draw": "^1.0.4",
        "leaflet.markercluster": "^1.4.1",
        "leaflet.wms": "^0.2.0",
        "mustache": "^4.0.0",
        "react-leaflet": "^2.5.0",
        "react-leaflet-draw": "^0.19.0"
    }
}
